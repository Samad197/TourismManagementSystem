        @*<div id="approvalBanner" class="alert alert-warning rounded-0 text-center d-none" role="status">
                Your account is <strong>pending approval</strong>. You can complete your profile and upload documents,
                but publishing tours/accepting bookings is disabled until approved.
                <a class="ms-2" href="@Url.Action("Profile", isAgency ? "Agency" : "Guide")">Complete Profile</a>
            </div>*@
        <script>
            (function () {
                function placeBanner() {
                    var banner = document.getElementById('approvalBanner');
                    if (!banner) return;

                    // Preferred: right after the page header/hero if present
                    var header = document.querySelector('.container-fluid.bg-breadcrumb');
                    if (header && header.parentNode) {
                        header.insertAdjacentElement('afterend', banner);
                        banner.classList.remove('d-none');
                        return;
                    }

                    // Fallback: place right under the navbar wrapper
                    var navWrap = document.querySelector('.container-fluid.position-relative.p-0');
                    if (navWrap && navWrap.parentNode) {
                        navWrap.insertAdjacentElement('afterend', banner);
                        banner.classList.remove('d-none');
                        return;
                    }

                    // Last resort
                    document.body.insertAdjacentElement('afterbegin', banner);
                    banner.classList.remove('d-none');
                }
                if (document.readyState === 'loading') {
                    document.addEventListener('DOMContentLoaded', placeBanner);
                } else {
                    placeBanner();
                }
            })();
        </script>
    }

