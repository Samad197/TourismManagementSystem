@using System.Security.Claims
@{
    // Auth
    var ci = User?.Identity as ClaimsIdentity;
    bool isAuth = ci?.IsAuthenticated ?? false;

    // Role
    string role = (ci?.FindFirst(ClaimTypes.Role)?.Value ?? ViewBag.RoleName as string ?? "").Trim();
    bool isAdmin = role.Equals("Admin", System.StringComparison.OrdinalIgnoreCase);
    bool isAgency = role.Equals("Agency", System.StringComparison.OrdinalIgnoreCase);
    bool isGuide = role.Equals("Guide", System.StringComparison.OrdinalIgnoreCase);
    bool isTourist = role.Equals("Tourist", System.StringComparison.OrdinalIgnoreCase);

    // Status (claims → ViewBag → Session)
    string claimApproved = ci?.FindFirst("isApproved")?.Value;
    string claimActive = ci?.FindFirst("isActive")?.Value;

    bool isApproved = (claimApproved != null && bool.TryParse(claimApproved, out var _ap)) ? _ap
                    : (ViewBag.IsApproved as bool?) ?? (Session["IsApproved"] as bool?) ?? false;

    bool isActive = (claimActive != null && bool.TryParse(claimActive, out var _ac)) ? _ac
                    : (ViewBag.IsActive as bool?) ?? (Session["IsActive"] as bool?) ?? false;

    // Preview switch
    var pv = Request["preview"];
    bool forcePublicNav = !string.IsNullOrEmpty(pv) &&
                          (pv == "1" || pv.Equals("true", System.StringComparison.OrdinalIgnoreCase) || pv.Equals("public", System.StringComparison.OrdinalIgnoreCase));

    // Provider management gate
    bool canManage = !forcePublicNav && (
        isAdmin ||
        (isAgency && isApproved && isActive) ||
        (isGuide && isApproved && isActive)
    );

    // Show public-facing links (About/Services/etc.) only for public/tourist/preview
    //bool showPublicLinks = forcePublicNav || !isAuth || isTourist;
    bool showPublicLinks =
    forcePublicNav || !isAuth || isTourist || (!isAgency && !isGuide && !isAdmin);


    // Active-class helper
    Func<string, string> active = name =>
string.Equals(ViewBag.ActivePage as string, name, System.StringComparison.OrdinalIgnoreCase) ? "active" : "";
}



